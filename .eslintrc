{
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "plugin:prettier/recommended",
        "./.eslintrc-auto-import.json"
    ],
    "rules": {
        "vue/multi-word-component-names": "off",
        "vue/html-self-closing": ["error", {
            "html": {
                "void": "always"
            }
        }]
    }
}
